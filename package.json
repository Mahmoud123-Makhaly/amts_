    "@next/bundle-analyzer": "^14.1.3",
    "@t3-oss/env-nextjs": "^0.9.2",
    "axios": "^1.6.7",
    "bootstrap": "5.3.3",
    "formik": "^2.4.5",
    "graphql": "^16.8.1",
    "immer": "^10.0.3",
    "moment": "^2.30.1",
    "next": "14.1.0",
    "next-auth": "^4.24.6",
    "next-intl": "^3.9.1",
    "next-safe-action": "^6.1.0",
    "primereact": "^10.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
