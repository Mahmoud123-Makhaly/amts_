{
  "name": "totplatform-next-template",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev --port 8098",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:docker": "make -f MakeFile build-$ENV_NAME",
    "start": "PORT=8098 node server.js",
