    "start:docker": "export NEXT_AUTH_SECRET=$(openssl rand -hex 64) && make -f MakeFile start-$ENV_NAME",
    "stop:docker": "make -f MakeFile stop-$ENV_NAME",
    "lint": "next lint"
  },
  "dependencies": {
    "@apollo/client": "^3.9.5",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
